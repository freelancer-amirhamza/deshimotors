{
    "version": 2,
    "name": "deshmotors",
    "builds": [
        {
            "src": "./index.html",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src" : "/(.*)",
            "dest": "/"
        }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ]
}